<?xml version="1.0"?>
<peripherals xmlns:xi="http://www.w3.org/2001/XInclude">
  <peripheral name="Uart" type="USART_TypeDef*">
    <item name="UART"/>
    <item name="USART"/>
    <interrupt/>
  </peripheral>
  <peripheral name="Spi" type="SPI_TypeDef*">
    <item name="SPI"/>
    <interrupt/>
  </peripheral>
  <peripheral name="I2c" type="I2C_TypeDef*">
    <item name="I2C"/>
    <interrupt name="Ev" postfix="_EV"/>
    <interrupt name="Er" postfix="_ER"/>
  </peripheral>
  <peripheral name="fdCan" type="FDCAN_GlobalTypeDef*">
    <item name="FDCAN"/>
    <interrupt name="It0" postfix="_IT0"/>
    <interrupt name="It1" postfix="_IT1"/>
  </peripheral>
  <peripheral name="Timer" type="TIM_TypeDef*" prefix="TIM">
    <item name="TIM1_8H5"/>
    <item name="TIM6_7H5"/>
  </peripheral>
  <peripheral name="LpTimer" type="LPTIM_TypeDef*" prefix="LPTIM">
    <item name="LPTIM"/>
    <interrupt/>
  </peripheral>
  <peripheral name="Adc" type="ADC_TypeDef*">
    <item name="ADC"/>
  </peripheral>
  <peripheral name="Dac" type="DAC_TypeDef*">
    <item name="DAC"/>
  </peripheral>
  <!-- <peripheral name="Rng" type="RNG_TypeDef*"><item name="RNG"/></peripheral> -->
  <!-- <peripheral name="Rtc" type="RTC_TypeDef*"><item name="RTC"/></peripheral> -->
</peripherals>
